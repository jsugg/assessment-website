{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "class-methods-use-this": "off",
        "no-plusplus": "off",
        "no-unused-vars": "warn",
        "curly": "error",
        "quotes": [
          "error",
          "double"
        ]
    }
}
